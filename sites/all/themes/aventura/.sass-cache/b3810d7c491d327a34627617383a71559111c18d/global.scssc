aside#region-sidebar-second section.block, 
aside#region-sidebar-first section.block, 
aside#region-sidebar-second div.block, 
aside#region-sidebar-first div.block {
	padding-left: 0;
}

.views-field-field-profile-image {
  width: 28%;
}

.views-field-field-bio {
  width: 72%;
}

#breadcrumb {
	margin-top: 0;
}

#region-footer-first,
#region-footer-second {
  width: 45%;
}

#region-footer-first {
  float: left;
  margin-right: 0;
}

#region-footer-second {
  float: right;
  margin-left: 0;
}
  
}

@media all and (max-width: 740px) {

body.front .region-content-inner {
  background: none;
  padding-top: 10%;
}

body.front .region-content-inner p {
  font-size: $em-base * 1.25;
}

p.two-column-first, div.two-column-first, span.two-column-first,
p.two-column-last, div.two-column-last, span.two-column-last {
	clear: both;
	width: 100%;
}

div.two-column-first p, 
div.two-column-last p {
	margin-bottom: 0;
	overflow: hidden;
}

select#superfish-1-select {
  font-size: 1em;
  width: $em-base * 14;
}

input[type="text"], textarea, select, input[type="date"], input[type="email"], 
input[type="password"], input[type="search"], input[type="tel"] {
  background: $base-color;
}

#zone-footer-wrapper {
  background: $contrast-color;
}

}

@media all and (max-width: 500px) {
	
#zone-user,
#zone-branding,
#zone-header,
#zone-preface,
#zone-content,
#zone-postscript,
#zone-footer{
	padding: 0 $em-base;
}

body.not-front #region-sidebar-second {
  padding-top: 0;
}

.views-field-field-image div a, 
.node-type-article .field-name-field-image {
  padding: 0;
}
	
.image-left,
.image-right {
  clear: both;
	float: left;
  margin: 0;
  max-width: 100%;
	width: 100%;
}

.caption p {
	text-align: left !important;
}

.views-field-field-profile-image {
  margin-bottom: $em-base;
  padding-right: 0;
  width: 100%;
}

.views-field-field-bio {
  clear: both;
  width: 100%;
}

#zone-branding.zone div.block-main-menu,
div.block-system-main-menu div.content select {
	clear: both;
	float: left;
	width: 100%;
}

body.not-front #region-branding {
	border-bottom: none;
}

#region-footer-first,
#region-footer-second {
  width: 100%;
}

#region-footer-first {
  margin-right: 10px;
}

#region-footer-second {
  float: left;
  margin-left: 10px;
}

.social-icons {
  text-align: left;
}

select#superfish-1-select {
  width: $em-base * 10;
}

#region-branding {
  margin-right: 0;
}

#region-menu {
  margin-left: 0;
}

h1.site-name, h1.site-name a, h2.site-name, h2.site-name a {
  margin-bottom: 0.2em;
}

#region-branding, #region-menu {
  max-width: 100%;
  width: 100%;
}

#region-menu {
  float: left;
  margin-left: $em-base;
  margin-bottom: 0.6em;
  text-align: left;
}

select#superfish-1-select {
  width: 80%;
  margin-bottom: 0.2em;
}

input[type="text"], textarea, select, input[type="date"], input[type="email"], 
input[type="password"], input[type="search"], input[type="tel"] {
  box-shadow: none;
  font-weight: normal;
  width: 80%;
}

}

